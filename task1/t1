<script type="text/javascript">
    numb1 = Number(promt('Введите первое число'));
    numb2 = Number(promt('Введите второе число'));
    if(numb1 > numb2){
        console.log(numb1);
    }
    if(numb1 < numb2){
        console.log(numb2);
    }
    if(numb1 == numb2){
        console.log('Значения равны между собой');
    }
</script>

