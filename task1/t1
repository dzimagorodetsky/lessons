for (i = 0; i < 100; i++){
array[i] = i**2
}

2.2)
function showarray ( ) {
let array = [];
for (i=0; i<100; i++){
array[i] = i**2
}
return array;
}

2.3)
function sum(array){
  let result= 0;
  for(let elements of array){
    result = result + elements;
  }
return result 
}
sum(array)

