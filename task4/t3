   let selectionElement = addElement('select', 'select-year', 'Выберите год');

            selectionElement.style.width = '400px';
            selectionElement.style.height = '30px';

            p.appendChild(selectionElement);
            let selectFind = document.querySelector('select');

            for (let i = 1960; i <= 2020; i++) {
                let new = addElement('option', '', String(i));
                selectFind.appendChild(new);
            }
